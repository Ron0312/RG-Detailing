---
import Layout from '../layouts/Layout.astro';
import BeforeAfterSlider from '../components/BeforeAfterSlider.jsx';
import PriceCalculator from '../components/PriceCalculator.jsx';
import Reviews from '../components/Reviews.astro';
import About from '../components/About.astro';
import { getCollection } from 'astro:content';
import { services } from '../data/services';
import { Gem, FlaskConical, Handshake, Droplets, Sparkles, ShieldCheck, Caravan, RefreshCw, Bike, Star, ArrowDown } from 'lucide-react';
import { Image } from 'astro:assets';
import heroImage from '../assets/rg-detailing_Fahrzeugaufbereitung_Tensfeld_Hero.jpg';

export const prerender = true;

const cities = await getCollection('cities');
const sortedCities = cities.sort((a, b) => a.data.name.localeCompare(b.data.name));

const iconMap = {
  0: Droplets,
  1: Sparkles,
  2: ShieldCheck,
  3: Caravan,
  4: RefreshCw,
  5: Bike
};

const mainSchema = {
  "@context": "https://schema.org",
  "@type": "AutoDetailing",
  "name": "RG Detailing",
  "image": "https://rg-detailing.de/logo.png",
  "description": "Exklusive Fahrzeugaufbereitung in Schleswig-Holstein. Zertifizierter Labocosmetica Partner für Keramikversiegelung, Lackveredelung & Leasing-Rückläufer in Tensfeld.",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "Dorfstraße 11",
    "addressLocality": "Tensfeld",
    "postalCode": "23824",
    "addressCountry": "DE",
    "addressRegion": "Schleswig-Holstein"
  },
  "geo": {
    "@type": "GeoCoordinates",
    "latitude": 54.0667,
    "longitude": 10.3000
  },
  "url": "https://rg-detailing.de",
  "telephone": "+491633845081",
  "priceRange": "€€-€€€",
  "openingHoursSpecification": [
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday"],
      "opens": "08:00",
      "closes": "18:00"
    },
    {
      "@type": "OpeningHoursSpecification",
      "dayOfWeek": ["Saturday"],
      "opens": "10:00",
      "closes": "14:00"
    }
  ],
  "areaServed": sortedCities.map(city => city.data.name).concat(["Schleswig-Holstein", "Bad Segeberg", "Lübeck", "Neumünster", "Kiel"]),
  "sameAs": [
      "https://www.instagram.com/rg_detailing",
      "https://www.facebook.com/rgdetailing"
  ],
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "High-End Detailing Services",
    "itemListElement": services.map((service, index) => ({
      "@type": "Offer",
      "itemOffered": {
        "@type": "Service",
        "name": service.title,
        "description": service.description,
        "url": `https://rg-detailing.de${service.link}`
      }
    }))
  },
  "makesOffer": services.map((service) => ({
      "@type": "Offer",
      "itemOffered": {
        "@type": "Service",
        "name": service.title,
        "description": service.description,
        "url": `https://rg-detailing.de${service.link}`,
        "provider": {
            "@type": "AutoDetailing",
            "name": "RG Detailing"
        }
      }
  })),
  "review": {
      "@type": "Review",
      "reviewRating": {
          "@type": "Rating",
          "ratingValue": "5",
          "bestRating": "5"
      },
      "author": {
          "@type": "Person",
          "name": "Google User"
      }
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "5.0",
    "reviewCount": "127"
  }
};

const faqSchema = {
  "@context": "https://schema.org",
  "@type": "FAQPage",
  "mainEntity": [
    {
      "@type": "Question",
      "name": "Bieten Sie auch eine reine Innenraumreinigung an?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Ja, wir bieten professionelle Innenraumreinigungen an. Unser \"Innen & Hygiene\" Paket beinhaltet eine Tiefenreinigung mit 170°C Trockendampf – ideal für Allergiker und zur Geruchsneutralisierung."
      }
    },
    {
      "@type": "Question",
      "name": "Was kostet eine Fahrzeugaufbereitung?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Die Kosten hängen vom Zustand und der Größe Ihres Fahrzeugs ab. Nutzen Sie unseren Preisrechner für eine individuelle Schätzung. Wir bieten Pakete von der einfachen Autoreinigung bis zum Full Detailing."
      }
    },
    {
      "@type": "Question",
      "name": "Machen Sie auch Motorwäsche?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Ja, als Teil unserer Spezial-Leistungen führen wir schonende Motorwäschen durch, bei denen empfindliche Elektronik geschützt wird."
      }
    },
    {
      "@type": "Question",
      "name": "Wie lange muss mein Auto bei Ihnen bleiben?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Je nach Paket zwischen 1 Tag (Innenraum/Basis) und 3-4 Tagen (Full Detailing mit Keramikversiegelung inkl. Aushärtezeit)."
      }
    }
  ]
};
---

<Layout
    title="RG Detailing | Premium Fahrzeugaufbereitung Schleswig-Holstein"
    description="Ihr Experte für Lackveredelung, Keramikversiegelung und Leasing-Aufbereitung. Zertifizierter Labocosmetica Partner in Tensfeld. Jetzt Termin sichern!"
    schema={[mainSchema, faqSchema]}
>

    {/* HERO SECTION */}
    <div class="relative min-h-screen flex items-center justify-center overflow-hidden bg-black">
        {/* High-End Hero Image */}
        <div class="absolute inset-0">
             <Image
                src={heroImage}
                alt="Premium Fahrzeugaufbereitung in der Werkstatt Tensfeld - RG Detailing"
                class="w-full h-full object-cover opacity-60"
                loading="eager"
                fetchpriority="high"
             />
             <div class="absolute inset-0 bg-gradient-to-b from-black/80 via-black/40 to-black"></div>
             {/* Subtle Red Overlay for Brand Identity */}
             <div class="absolute inset-0 bg-red-900/10 mix-blend-overlay"></div>
        </div>

        {/* Red Accents */}
        <div class="absolute top-0 right-0 w-[50vw] h-[50vw] bg-red-600/10 blur-[150px] rounded-full animate-pulse"></div>
        <div class="absolute bottom-0 left-0 w-[30vw] h-[30vw] bg-red-900/10 blur-[100px] rounded-full"></div>

        <div class="container mx-auto px-4 z-10 text-center pt-20">
            <h2 class="inline-block py-2 px-4 rounded-full bg-white/5 border border-white/10 text-zinc-300 text-xs md:text-sm font-bold tracking-[0.2em] mb-8 uppercase backdrop-blur-md">
                Handwerk. Leidenschaft. Perfektion.
            </h2>
            <h1 class="text-6xl md:text-8xl font-bold mb-8 text-white tracking-tighter leading-tight">
                Wellness für <br/>
                <span class="text-transparent bg-clip-text bg-gradient-to-r from-red-500 via-red-600 to-red-800">Ihr Automobil</span>
            </h1>
            <p class="text-xl md:text-2xl text-zinc-400 max-w-2xl mx-auto mb-12 font-light leading-relaxed">
                Professionelle <strong class="text-white">Fahrzeugaufbereitung</strong>, <strong class="text-white">Autoreinigung</strong> und <strong class="text-white">Lackveredelung</strong> in Schleswig-Holstein.<br class="hidden md:block"/>
                Erleben Sie den Unterschied zwischen Waschanlage und echtem Detailing.
            </p>
            <div class="flex flex-col md:flex-row justify-center gap-6">
                <a href="#rechner" class="group relative px-8 py-4 bg-red-700 rounded-full font-bold text-white overflow-hidden transition-all hover:scale-105 hover:shadow-[0_0_40px_-10px_rgba(220,38,38,0.5)]">
                    <span class="relative z-10">Fahrzeugwert berechnen</span>
                    <div class="absolute inset-0 bg-gradient-to-r from-red-600 to-red-500 opacity-0 group-hover:opacity-100 transition-opacity"></div>
                </a>
                <a href="#leistungen" class="px-8 py-4 rounded-full border border-white/10 bg-white/5 text-white font-bold hover:bg-white/10 transition-colors backdrop-blur-sm">
                    Unsere Leistungen
                </a>
            </div>
        </div>

        {/* Scroll Indicator */}
        <div class="absolute bottom-10 left-1/2 transform -translate-x-1/2 animate-bounce text-zinc-500">
             <ArrowDown className="w-6 h-6" />
        </div>
    </div>

    {/* TRUST SIGNALS STRIP */}
    <div class="border-y border-white/5 bg-zinc-950/50 backdrop-blur-sm relative z-20">
        <div class="container mx-auto px-4 py-8">
            <div class="flex flex-wrap justify-center gap-8 md:gap-20 opacity-80 grayscale-0 transition-all duration-500">
                {/* Partners Text/Logos */}
                <span class="text-lg md:text-xl font-bold text-zinc-300 flex items-center gap-3">
                    <Star className="text-red-600 w-6 h-6 fill-red-600" /> Labocosmetica Certified
                </span>
                <span class="text-lg md:text-xl font-bold text-zinc-300 flex items-center gap-3">
                    <Star className="text-red-600 w-6 h-6 fill-red-600" /> DAMPFTEC System
                </span>
                <span class="text-lg md:text-xl font-bold text-zinc-300 flex items-center gap-3">
                    <Star className="text-red-600 w-6 h-6 fill-red-600" /> 5-Sterne Service
                </span>
            </div>
        </div>
    </div>

    <main>
        <About />

        {/* WHY US SECTION */}
        <section class="py-20 bg-zinc-900 border-y border-white/5">
            <div class="container mx-auto px-4">
                <h2 class="text-3xl md:text-4xl font-bold text-white mb-12 text-center">Warum RG Detailing Tensfeld?</h2>
                <div class="grid md:grid-cols-3 gap-8">
                    <div class="text-center p-6 rounded-2xl bg-black/20 border border-white/5 hover:border-red-900/50 transition duration-300 flex flex-col items-center">
                        <div class="mb-4 text-red-500">
                            <Gem size={48} strokeWidth={1.5} />
                        </div>
                        <h3 class="text-xl font-bold text-white mb-2">Zertifizierte Keramikversiegelung</h3>
                        <p class="text-zinc-400">Als Labocosmetica Certified Detailer garantieren wir höchste Standards bei Lackschutz und Standzeit.</p>
                    </div>
                    <div class="text-center p-6 rounded-2xl bg-black/20 border border-white/5 hover:border-red-900/50 transition duration-300 flex flex-col items-center">
                        <div class="mb-4 text-red-500">
                            <FlaskConical size={48} strokeWidth={1.5} />
                        </div>
                        <h3 class="text-xl font-bold text-white mb-2">High-End Pflegeprodukte</h3>
                        <p class="text-zinc-400">Wir nutzen exklusive Chemie (z.B. Labocosmetica, Gtechniq) für maximalen Glanz, den man in Schleswig-Holstein selten findet.</p>
                    </div>
                    <div class="text-center p-6 rounded-2xl bg-black/20 border border-white/5 hover:border-red-900/50 transition duration-300 flex flex-col items-center">
                        <div class="mb-4 text-red-500">
                            <Handshake size={48} strokeWidth={1.5} />
                        </div>
                        <h3 class="text-xl font-bold text-white mb-2">Norddeutsch & Direkt</h3>
                        <p class="text-zinc-400">Keine leeren Versprechen. Wir erklären genau, was bei Ihrer Aufbereitung möglich ist – und was nicht.</p>
                    </div>
                </div>
            </div>
        </section>

        {/* SERVICES SECTION */}
        <section id="leistungen" class="py-32 relative">
             <div class="container mx-auto px-4">
                <div class="text-center mb-20">
                    <span class="text-red-500 font-bold tracking-widest text-sm uppercase">Ihr Fahrzeug in besten Händen</span>
                    <h2 class="text-4xl md:text-5xl font-bold mt-4 mb-6 text-white">Das High-End Portfolio</h2>
                    <p class="text-zinc-400 max-w-2xl mx-auto">
                        Von der hygienischen <strong>Innenreinigung</strong> mit Dampf bis zur mehrstufigen <strong>Lackpolitur</strong>.
                        Wir sind Ihr Ansprechpartner für Werterhalt in der Region Segeberg.
                    </p>
                </div>

                <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
                    {services.map((service, index) => {
                        const IconComponent = iconMap[index] || Sparkles;
                        return (
                        <a href={service.link} class="group glass-card p-10 rounded-3xl relative overflow-hidden hover:-translate-y-2 transition-transform duration-500 block">
                             <div class="absolute top-0 right-0 w-32 h-32 bg-gradient-to-br from-red-600/20 to-transparent rounded-bl-full -mr-10 -mt-10 transition-all group-hover:scale-150"></div>

                             <div class="relative z-10">
                                <div class="w-14 h-14 bg-zinc-900 rounded-xl border border-white/10 flex items-center justify-center text-white mb-8 group-hover:border-red-500/50 transition-colors shadow-lg group-hover:text-red-500">
                                    <IconComponent size={24} strokeWidth={1.5} />
                                </div>
                                <h3 class="text-2xl font-bold text-white mb-4 group-hover:text-red-400 transition-colors">{service.title}</h3>
                                <p class="text-zinc-400 leading-relaxed mb-6">{service.shortDescription}</p>
                                <div class="flex items-center text-sm font-bold text-zinc-300 group-hover:text-white">
                                    Mehr erfahren <span class="ml-2 transform group-hover:translate-x-1 transition-transform">→</span>
                                </div>
                             </div>
                        </a>
                        );
                    })}
                </div>
            </div>
        </section>

        {/* PROCESS SECTION */}
        <section class="py-20 relative overflow-hidden">
             <div class="container mx-auto px-4">
                 <h2 class="text-3xl md:text-4xl font-bold text-white mb-16 text-center">Der RG Detailing Ablauf</h2>
                 <div class="grid md:grid-cols-4 gap-4 relative">
                     <div class="hidden md:block absolute top-12 left-0 w-full h-0.5 bg-gradient-to-r from-zinc-800 via-red-900 to-zinc-800 -z-10"></div>
                     {/* Steps 1-4 */}
                     <div class="relative bg-zinc-950/80 p-6 rounded-xl border border-zinc-800 text-center backdrop-blur-sm hover:border-red-900/50 transition">
                         <div class="w-12 h-12 bg-red-700 rounded-full flex items-center justify-center text-white font-bold text-xl mx-auto mb-4 border-4 border-zinc-950 shadow-[0_0_20px_rgba(220,38,38,0.5)]">1</div>
                         <h3 class="text-white font-bold mb-2">Check</h3>
                         <p class="text-sm text-zinc-400">Bestandsaufnahme & Lackanalyse unter Speziallicht in unserer Halle.</p>
                     </div>
                     <div class="relative bg-zinc-950/80 p-6 rounded-xl border border-zinc-800 text-center backdrop-blur-sm hover:border-red-900/50 transition">
                         <div class="w-12 h-12 bg-zinc-800 rounded-full flex items-center justify-center text-white font-bold text-xl mx-auto mb-4 border-4 border-zinc-950">2</div>
                         <h3 class="text-white font-bold mb-2">Wash</h3>
                         <p class="text-sm text-zinc-400">Schonende Handwäsche, Flugrostentfernung & Kneten.</p>
                     </div>
                     <div class="relative bg-zinc-950/80 p-6 rounded-xl border border-zinc-800 text-center backdrop-blur-sm hover:border-red-900/50 transition">
                         <div class="w-12 h-12 bg-zinc-800 rounded-full flex items-center justify-center text-white font-bold text-xl mx-auto mb-4 border-4 border-zinc-950">3</div>
                         <h3 class="text-white font-bold mb-2">Polish</h3>
                         <p class="text-sm text-zinc-400">Defektkorrektur: Kratzer entfernen für ultimativen Tiefenglanz.</p>
                     </div>
                     <div class="relative bg-zinc-950/80 p-6 rounded-xl border border-zinc-800 text-center backdrop-blur-sm hover:border-red-900/50 transition">
                         <div class="w-12 h-12 bg-zinc-800 rounded-full flex items-center justify-center text-white font-bold text-xl mx-auto mb-4 border-4 border-zinc-950">4</div>
                         <h3 class="text-white font-bold mb-2">Protect</h3>
                         <p class="text-sm text-zinc-400">Langzeitversiegelung mit Keramik, Graphen oder Wachs.</p>
                     </div>
                 </div>
             </div>
        </section>

        {/* COMPARISON SLIDER */}
        <section class="py-20 bg-zinc-900/30 border-y border-white/5">
            <div class="container mx-auto px-4">
                 <div class="flex flex-col md:flex-row items-center gap-12">
                     <div class="md:w-1/3">
                         <h2 class="text-4xl font-bold mb-6 text-white">Wahrheit <br/> statt Versprechen</h2>
                         <p class="text-zinc-400 mb-8 text-lg leading-relaxed">
                             Fotos können lügen, aber dieser Schieberegler nicht.
                             Sehen Sie selbst, was eine professionelle <strong>Lackkorrektur</strong> bewirken kann.
                             Grauschleier verschwinden, tiefe Farbe kehrt zurück.
                         </p>
                         <div class="flex gap-4">
                             <div class="text-center">
                                 <div class="text-3xl font-bold text-red-500">85%</div>
                                 <div class="text-xs text-zinc-500 uppercase tracking-wider">Defekt-Korrektur</div>
                             </div>
                             <div class="text-center">
                                 <div class="text-3xl font-bold text-red-500">100%</div>
                                 <div class="text-xs text-zinc-500 uppercase tracking-wider">Tiefenglanz</div>
                             </div>
                         </div>
                     </div>
                     <div class="md:w-2/3 w-full">
                        <div class="rounded-2xl overflow-hidden shadow-2xl border border-zinc-700">
                             <BeforeAfterSlider
                                client:visible
                                beforeImage="https://rg-detailing.de/wp-content/uploads/2023/01/Fahrzeugaufbereitung-vorher-nacher-1.jpg"
                                afterImage="https://rg-detailing.de/wp-content/uploads/2023/01/Fahrzeugaufbereitung-vorher-nacher-2.jpg"
                                alt="Fahrzeugaufbereitung Vorher Nachher Vergleich"
                            />
                        </div>
                     </div>
                 </div>
            </div>
        </section>

        {/* CALCULATOR */}
        <section id="rechner" class="py-32 relative overflow-hidden">
             <div class="absolute inset-0 bg-red-900/5 z-0"></div>
             <div class="container mx-auto px-4 relative z-10">
                 <div class="max-w-4xl mx-auto glass-panel p-8 md:p-16 rounded-[3rem] shadow-2xl border border-red-500/10">
                    <div class="text-center mb-12">
                        <h2 class="text-3xl md:text-5xl font-bold mb-6 text-white">Kostenrechner Aufbereitung</h2>
                        <p class="text-zinc-400">Konfigurieren Sie Ihr Paket. Fair, transparent und ohne versteckte Kosten.</p>
                    </div>
                    <PriceCalculator client:visible />
                 </div>
             </div>
        </section>

        {/* GALLERY GRID */}
        <section class="py-20">
             <div class="container mx-auto px-4">
                <h2 class="text-3xl font-bold text-white mb-8 text-center">Ergebnisse aus unserer Werkstatt</h2>
                <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-5 gap-4">
                    <div class="aspect-square bg-zinc-800 rounded-xl overflow-hidden group">
                        <img src="/images/gallery/fahrzeugaufbereitung-tensfeld-lackveredelung-01.png" alt="Lackveredelung und Fahrzeugaufbereitung Ergebnis in Tensfeld" class="w-full h-full object-cover group-hover:scale-110 transition duration-700" loading="lazy" width="800" height="800" />
                    </div>
                    <div class="aspect-square bg-zinc-800 rounded-xl overflow-hidden group">
                        <img src="/images/gallery/fahrzeugaufbereitung-tensfeld-innenreinigung-02.jpg" alt="Professionelle Innenraumreinigung Vorher-Nachher RG Detailing" class="w-full h-full object-cover group-hover:scale-110 transition duration-700" loading="lazy" width="800" height="800" />
                    </div>
                    <div class="aspect-square bg-zinc-800 rounded-xl overflow-hidden group">
                        <img src="/images/gallery/fahrzeugaufbereitung-tensfeld-keramikversiegelung-03.jpg" alt="Keramikversiegelung Hochglanz Ergebnis Labocosmetica" class="w-full h-full object-cover group-hover:scale-110 transition duration-700" loading="lazy" width="800" height="800" />
                    </div>
                    <div class="aspect-square bg-zinc-800 rounded-xl overflow-hidden group">
                         <img src="/images/gallery/fahrzeugaufbereitung-tensfeld-vorher-nachher-04.jpg" alt="Fahrzeugaufbereitung Detailarbeit Ergebnis" class="w-full h-full object-cover group-hover:scale-110 transition duration-700" loading="lazy" width="800" height="800" />
                    </div>
                    <div class="aspect-square bg-zinc-800 rounded-xl overflow-hidden group">
                         <img src="/images/gallery/fahrzeugaufbereitung-tensfeld-detailing-05.jpg" alt="Exklusives Detailing Ergebnis Schleswig-Holstein" class="w-full h-full object-cover group-hover:scale-110 transition duration-700" loading="lazy" width="800" height="800" />
                    </div>
                </div>
             </div>
        </section>

        {/* FAQ SECTION FOR SEO */}
        <section class="py-20 border-t border-white/5">
            <div class="container mx-auto px-4">
                <div class="max-w-3xl mx-auto">
                    <h2 class="text-3xl font-bold text-white mb-8 text-center">Häufige Fragen zur Fahrzeugaufbereitung</h2>
                    <div class="space-y-6">
                        <div class="glass-card p-6 rounded-2xl">
                            <h3 class="text-xl font-bold text-white mb-2">Bieten Sie auch eine reine Innenraumreinigung an?</h3>
                            <div>
                                <p class="text-zinc-400">Ja, wir bieten professionelle Innenraumreinigungen an. Unser "Innen & Hygiene" Paket beinhaltet eine Tiefenreinigung mit 170°C Trockendampf – ideal für Allergiker und zur Geruchsneutralisierung.</p>
                            </div>
                        </div>
                        <div class="glass-card p-6 rounded-2xl">
                            <h3 class="text-xl font-bold text-white mb-2">Was kostet eine Fahrzeugaufbereitung?</h3>
                            <div>
                                <p class="text-zinc-400">Die Kosten hängen vom Zustand und der Größe Ihres Fahrzeugs ab. Nutzen Sie unseren <a href="#rechner" class="text-red-500 hover:underline">Preisrechner</a> für eine individuelle Schätzung. Wir bieten Pakete von der einfachen Autoreinigung bis zum Full Detailing.</p>
                            </div>
                        </div>
                        <div class="glass-card p-6 rounded-2xl">
                            <h3 class="text-xl font-bold text-white mb-2">Machen Sie auch Motorwäsche?</h3>
                            <div>
                                <p class="text-zinc-400">Ja, als Teil unserer Spezial-Leistungen führen wir schonende Motorwäschen durch, bei denen empfindliche Elektronik geschützt wird.</p>
                            </div>
                        </div>
                        <div class="glass-card p-6 rounded-2xl">
                            <h3 class="text-xl font-bold text-white mb-2">Wie lange muss mein Auto bei Ihnen bleiben?</h3>
                            <div>
                                <p class="text-zinc-400">Je nach Paket zwischen 1 Tag (Innenraum/Basis) und 3-4 Tagen (Full Detailing mit Keramikversiegelung inkl. Aushärtezeit).</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        {/* CITIES SEO GRID */}
        <section class="py-20 border-t border-white/5">
             <div class="container mx-auto px-4 text-center">
                 <h3 class="text-zinc-500 font-bold uppercase tracking-widest text-sm mb-8">Fahrzeugaufbereitung in Ihrer Nähe</h3>
                 <div class="flex flex-wrap justify-center gap-4 max-w-4xl mx-auto">
                    {sortedCities.map(city => (
                        <a href={`/${city.data.slug}`} class="px-6 py-3 rounded-xl bg-zinc-900 border border-zinc-800 text-zinc-400 hover:text-white hover:border-red-500/50 hover:bg-zinc-800 transition-all text-sm font-medium">
                            Aufbereitung {city.data.name}
                        </a>
                    ))}
                 </div>
             </div>
        </section>

    </main>
</Layout>
